_:addButton("Trigger Bot - test", function()
    while true do
        for _, enemy in ipairs(game.Workspace:GetDescendants()) do
            if enemy:IsA("BasePart") and enemy.CanCollide then
                local character = game.Players.LocalPlayer.Character
                if character then
                    local tool = character:FindFirstChild("Tool")
                    if tool then
                        local mouse = game.Players.LocalPlayer:GetMouse()
                        if mouse then
                            local hit, pos = mouse.Hit.p, mouse.Hit
                            if hit then
                                local direction = (hit - tool.Parent.HumanoidRootPart.Position).Unit
                                tool.Parent.Gun.FireServer(tool, pos, direction)
                            end
                        end
                    end
                end
            end
        end
        wait(0.1)
    end
end)

