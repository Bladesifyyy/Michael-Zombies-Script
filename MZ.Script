g:addToggle(
    "Auto Heal",  -- label text
    function(c)  -- callback function
        if c == true then
            while true do
                local char = game.Players.LocalPlayer.Character
                local humanoid = char:FindFirstChildWhichIsA("Humanoid")
                
                -- check if character and humanoid exist
                if humanoid and humanoid.Health < 100 then
                    -- heal the character if its health is less than 100
                    humanoid:Heal()
                end
                
                wait(1)  -- wait for 1 second before checking again
            end
        end
    end
)

