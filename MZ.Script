mt:addToggle("Auto Farm", function(c)
    if c then
        while wait(1) do
            for _, v in ipairs(game.Workspace["\101\120\116\101\114\110\105\115\116\101\109\111\118\101\100"].ZombieSpawner:GetChildren()) do
                if v:FindFirstChild("HumanoidRootPart") and v.HumanoidRootPart:FindFirstChild("Humanoid") then
                    local distance = (l.HumanoidRootPart.Position - v.HumanoidRootPart.Position).Magnitude
                    if distance <= 20 then
                        local success, err = pcall(function()
                            v.HumanoidRootPart.CFrame = l.HumanoidRootPart.CFrame * CFrame.new(0, 0, -3)
                            wait(0.5)
                        end)
                        if not success then
                            warn("Error moving to zombie: ", err)
                        end
                    end
                end
            end
        end
    else
        -- disable 
    end
end)

mt:addButton("Auto Money", function()
    while wait(1) do
        -- Find money on map
        local money = game.Workspace:FindFirstChild(MONEY_SPAWN):FindFirstChild(MONEY_ITEM)
        if money then
            -- Check if player has MoneyFromServer item in backpack
            local backpack = d.Backpack
            local moneyFromServer = backpack:FindFirstChild("MoneyFromServer")
            if moneyFromServer then
                -- Player has MoneyFromServer item, pick up money
                local success, error_message = pcall(function()
                    moneyFromServer.Parent = d.Character
                    wait(0.1)
                    d.Character:FindFirstChild("MoneyFromServer"):Destroy()
                end)
                if not success then
                    warn("Error picking up money: " .. error_message)
                end
            else
                -- Player does not have MoneyFromServer item, do not pick up money
                warn("Player does not have MoneyFromServer item in backpack, cannot pick up money.")
            end
        else
            warn("Money not found on map.")
        end
    end
end)

